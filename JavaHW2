class Main {
  public static void main(String[] args) {
  
int k = 80;
for(int i = 0; i < 5; i++) {
k = k - 12;
} 
System.out.println(k);

for (int i = 0; i <= 4; i++) {
            for (int j = 0; j <= i; j++) {
                System.out.print("*");
            }

            for (int x = 0; x <= 1; x++) {
                System.out.println(" ");
            }
        }
        System.out.println();
  }  
  }
